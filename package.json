{
  "name": "ng-svg-icon-sprite",
  "version": "0.7.1",
  "description": "Angular 4+ package for generating and using inline SVG icons in your project",
  "license": "MIT",
  "author": "Jan Suwart",
  "bugs": {
    "url": "https://github.com/jannicz/ng-svg-icon-sprite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jannicz/ng-svg-icon-sprite.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-github-docs": "ng build --prod --output-path docs --base-href ng-svg-icon-sprite",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "create-package": "ng-packagr -p ng-package.json",
    "create-icon-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/sprite.svg --stripAttrs fill --stripAttrs stroke",
    "create-image-sprite": "svg2sprite ./src/assets/svg-images ./src/assets/sprites/image-sprite.svg"
  },
  "dependencies": {
    "@angular/animations": ">4.0.0",
    "@angular/common": ">4.0.0",
    "@angular/compiler": ">4.0.0",
    "@angular/core": ">4.0.0",
    "@angular/forms": ">4.0.0",
    "@angular/http": ">4.0.0",
    "@angular/platform-browser": ">4.0.0",
    "@angular/platform-browser-dynamic": ">4.0.0",
    "@angular/router": ">4.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.2",
    "svg2sprite-cli": "2.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": ">4.0.0",
    "@angular/language-service": ">4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2",
    "svg4everybody": "^2.1.9"
  }
}
